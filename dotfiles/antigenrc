  # the following must be last, and must be in this order
  zsh-users/zsh-syntax-highlighting
  zsh-users/zsh-history-substring-search
EOBUNDLES

# Load the theme.
antigen theme agnoster

# Tell Antigen that you're done.
antigen apply
